# Creating Date_Monthly and CPI_monthly columns
head(cpi.df1 <- cpi.df %>%
group_by(month(Date_daily), year(Date_daily)) %>%
mutate(Date_monthly1 = paste0(year(Date_daily),"-",month(Date_daily),"-01"),
